
@{
    ViewBag.Title = "ResetPassword";
    Layout = "~/Views/Shared/_EmptyLayout.cshtml";
}

<div id="overlay" class="hideEle">
</div>
<div id="reset-page" class="reset-page showEle">
    <div class="form">
        <div class="reset-form">
            <input type="password" id="reset-password" placeholder="reset password" />
            <input type="password" id="reset-reenter-password" placeholder="re-enter password" />
            <button id="resetpasswordBtn">Reset Password</button>
            <div class="spacer"></div>
            <a href="Index.html">Back to Login ...</a>
            <div class="spacer"></div>
            <p class="error-message" id="reset-error"></p>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.2.1.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/index.css" rel="stylesheet" />
<script src="~/PageJS/common.js" type="text/javascript"></script>
<script src="~/PageJS/resetpassword.js"></script>

